{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn gen:graphql:dev && next dev",
    "build": "yarn gen:graphql:prod && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix && yarn run prettier:fix",
    "prettier": "prettier '**/*.*(md|json|yaml|ts|js|tsx)' --check --cache --log-level warn",
    "prettier:fix": "yarn run prettier '**/*.*(md|json|yaml|ts|js|tsx)' --write --cache --log-level=warn",
    "gen:graphql:dev": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config",
    "gen:graphql:prod": "DOTENV_CONFIG_PATH=./.env graphql-codegen -r dotenv/config",
    "postinstall": "prisma generate && husky install",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.11.0-rc.2",
    "@apollo/experimental-nextjs-app-support": "^0.8.0",
    "@auth/prisma-adapter": "^2.4.2",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/system": "^2.6.2",
    "@codeium/react-code-editor": "^1.0.12",
    "@duneanalytics/client-sdk": "^0.2.5",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@nikolovlazar/chakra-ui-prose": "^1.2.1",
    "@octokit/rest": "^21.0.0",
    "@prisma/client": "^5.17.0",
    "@rainbow-me/rainbowkit": "^2.2.0",
    "@reown/walletkit": "^1.1.1",
    "@tanstack/react-query": "^5.59.15",
    "@types/papaparse": "^5.3.14",
    "@types/tinycolor2": "^1.4.6",
    "@walletconnect/core": "^2.19.2",
    "@walletconnect/utils": "^2.19.2",
    "axios": "^1.8.4",
    "bignumber.js": "^9.1.2",
    "chakra-react-select": "^4.9.1",
    "date-fns": "^3.6.0",
    "encoding": "^0.1.13",
    "ethers": "^6.13.5",
    "ethers-multicall": "^0.2.3",
    "framer-motion": "^11.3.0",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "limiter": "^2.1.0",
    "lodash": "^4.17.21",
    "next": "15.2.5",
    "next-auth": "^5.0.0-beta.20",
    "next-themes": "^0.3.0",
    "papaparse": "^5.4.1",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.3.3",
    "react": "^18",
    "react-confetti": "^6.1.0",
    "react-dom": "^18",
    "react-feather": "^2.0.10",
    "react-icons": "^5.2.1",
    "react-json-view": "^1.21.3",
    "recharts": "^2.15.2",
    "tinycolor2": "^1.6.0",
    "use-debounce": "^10.0.4",
    "viem": "^2.24.2",
    "wagmi": "^2.14.16"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "15.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yaml}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  }
}
